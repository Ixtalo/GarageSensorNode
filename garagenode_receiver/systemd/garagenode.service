[Unit]
Description=Garage Node Receiver MQTT


[Service]
Type=simple
ExecStart=/usr/bin/python3 /mnt/data/mydata/garagenode/garagenode_receiver_mqtt.py /dev/ttyAMA0 9600
Nice=-5
User=ast
Restart=on-failure
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=garagenode


[Install]
WantedBy=multi-user.target
